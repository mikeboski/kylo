{
  "name": "kylo",
  "version": "0.9.0-1",
  "private": true,
  "description": "kylo",
  "scripts": {
    "build": "tsc -p src/main/resources/static/tsconfig.app.json",
    "start": "tsc -p src/main/resources/static/tsconfig.app.json && concurrently \"tsc -p src/main/resources/static -w\" \"lite-server -c src/main/resources/static/bs-config.js\"",
    "reinstall": "rm -rf node_modules tmp deploy dist && npm i",
    "test": "karma start karma.conf.js",
    "test:watch": "tsc -p src/main/resources/static/tsconfig.app.json && concurrently \"tsc -p src/main/resources/static/tsconfig.spec.json -w\" \"karma start karma.conf.js\"",
    "build-css": "node-sass --include-path scss ./src/main/resources/static/assets/medikly_theme/styles/mdk-main.scss ./src/main/resources/static/assets/medikly_theme/styles/css/mdk-main.css",
    "build-css-pricing": "node-sass --include-path scss ./src/main/resources/static/assets/medikly_theme/styles/scss/components/_pricing.scss ./src/main/resources/static/assets/medikly_theme/styles/css/pricing.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\""
  },
  "repository": "https://github.com/teradata/kylo.git",
  "license": "Apache-2.0",
  "author": "Think Big Analytics",
  "dependencies": {
    "@agm/core": "1.0.0-beta.2",
    "@angular-devkit/core": "^0.3.2",
    "@angular/animations": "~5.2",
    "@angular/cdk": "~5.2",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "~5.2",
    "@angular/core": "~5.2",
    "@angular/flex-layout": "^5.0.0-beta.14",
    "@angular/forms": "~5.2",
    "@angular/http": "~5.2",
    "@angular/material": "~5.2",
    "@angular/platform-browser": "~5.2",
    "@angular/platform-browser-dynamic": "~5.2",
    "@angular/router": "~5.2",
    "@angular/upgrade": "~5.2",
    "@covalent/core": "^1.0.0-rc.1",
    "@covalent/markdown": "^1.0.0",
    "@covalent/text-editor": "^1.0.0",
    "@qontu/ngx-inline-editor": "^0.2.0-alpha.12",
    "@swimlane/ngx-datatable": "^11.3.2",
    "@types/acorn": "^4.0.3",
    "@uirouter/angular-hybrid": "^4.0.0",
    "angular": "^1.6.7",
    "angular-translate": "^2.17.0",
    "angular2-autosize": "^1.0.2",
    "angular2-highcharts": "^0.5.5",
    "core-js": "^2.5.3",
    "jasmine": "^3.1.0",
    "moment": "2.22.1",
    "ng-circle-progress": "^0.9.9",
    "ng2-dragula": "^1.5.0",
    "ng2-file-upload": "^1.3.0",
    "ng2-validation": "^4.2.0",
    "ngx-order-pipe": "^2.0.1",
    "ngx-perfect-scrollbar": "^5.3.5",
    "rxjs": "^5.5.5",
    "showdown": "^1.8.6",
    "simplemde": "^1.11.2",
    "systemjs": "^0.21.2",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "^0.3.2",
    "@angular/cli": "^1.4.9",
